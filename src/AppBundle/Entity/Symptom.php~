<?php
// src\AppBundle\Entity\Symptom.php
namespace AppBundle\Entity;
use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 */
class Symptom {
    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @ORM\ManyToMany(targetEntity="Disease", mappedBy="symptom")
     */
    protected $disease;

    /**
     * @ORM\Column(type="string", length=255)
     */
    protected $name;

    /**
    * @ORM\Column(type="text")
    */
    protected $description;

    /**
     * @ORM\Column(type="string", length=255);
     */
    protected $tags;

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return Symptom
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }
    /**
     * Constructor
     */
    public function __construct()
    {
        $this->disease = new \Doctrine\Common\Collections\ArrayCollection();
    }

    /**
     * Add disease
     *
     * @param \AppBundle\Entity\Disease $disease
     * @return Symptom
     */
    public function addDisease(\AppBundle\Entity\Disease $disease)
    {
        $this->disease[] = $disease;

        return $this;
    }

    /**
     * Remove disease
     *
     * @param \AppBundle\Entity\Disease $disease
     */
    public function removeDisease(\AppBundle\Entity\Disease $disease)
    {
        $this->disease->removeElement($disease);
    }

    /**
     * Get disease
     *
     * @return \Doctrine\Common\Collections\Collection
     */
    public function getDisease()
    {
        return $this->disease;
    }
}
