{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Diagnosis</h1>

    <div class="table-responsive">
		<table class="table table-striped">
			<tbody>
				<tr>
					<th>Id</th>
					<td>{{ entity.id }}</td>
				</tr>
				<tr>
					<th>Date</th>
					<td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
				</tr>
				<tr>
					<th>Prescription</th>
					<td>{{ entity.prescription }}</td>
				</tr>
			</tbody>
		</table>
	</div>

        <ul class="record_actions">
    <li>
        <a href="{{ path('diagnosis') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('diagnosis_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
